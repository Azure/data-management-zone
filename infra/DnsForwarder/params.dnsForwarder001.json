{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": "@{value=<my-region>}",
    "vmssName": "@{value=<my-data-hub-name>dnsproxy001}",
    "vmssSkuTier": "@{value=Standard}",
    "vmssSkuName": "@{value=Standard_A1_v2}",
    "vmssAdminUsername": "@{value=VmssMainUser}",
    "storageAccountId": "@{value=/subscriptions/<my-data-hub-subscription-id>/resourceGroups/<my-data-hub-name>-network/providers/Microsoft.Storage/storageAccounts/<my-data-hub-name>artifactstorage001}",
    "storageAccountContainerName": "@{value=scripts}",
    "subnetId": "@{value=/subscriptions/<my-data-hub-subscription-id>/resourceGroups/<my-data-hub-name>-network/providers/Microsoft.Network/virtualNetworks/<my-data-hub-name>-vnet/subnets/<my-data-hub-name>-subnet}"
