{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "containerRegistryName": {
      "value": "mydatahubnamecontainerregistry001"
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/mydatahubname-network/providers/Microsoft.Network/virtualNetworks/mydatahubname-vnet/subnets/mydatahubname-privatelink-subnet"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/mydatahubname-global-dns/providers/Microsoft.Network/privateDnsZones/privatelink.azurecr.io"
    }
  }
}
