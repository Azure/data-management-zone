{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "containerRegistryName": {
      "value": "<my-data-hub-name>containerregistry001"
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/<my-data-hub-name>-network/providers/Microsoft.Network/virtualNetworks/<my-data-hub-name>-vnet/subnets/<my-data-hub-name>-privatelink-subnet"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/<my-data-hub-name>-global-dns/providers/Microsoft.Network/privateDnsZones/privatelink.azurecr.io"
    }
  }
}
