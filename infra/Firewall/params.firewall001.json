{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{regionName}"
    },
    "firewallName": {
      "value": "dataHubName-firewall"
    },
    "firewallPolicyId": {
      "value": "/subscriptions/{dataHubSubscriptionId}/resourceGroups/dataHubName-network/providers/Microsoft.Network/firewallPolicies/dataHubName-firewallpolicy"
    },
    "subnetId": {
      "value": "/subscriptions/{dataHubSubscriptionId}/resourceGroups/dataHubName-network/providers/Microsoft.Network/virtualNetworks/dataHubName-vnet/subnets/AzureFirewallSubnet"
    }
  }
}
