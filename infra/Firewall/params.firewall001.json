{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "firewallName": {
      "value": "mydatahubname-firewall"
    },
    "firewallPolicyId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/mydatahubname-network/providers/Microsoft.Network/firewallPolicies/mydatahubname-firewallpolicy"
    },
    "subnetId": {
      "value": "/subscriptions/<my-data-hub-subscription-id>/resourceGroups/mydatahubname-network/providers/Microsoft.Network/virtualNetworks/mydatahubname-vnet/subnets/AzureFirewallSubnet"
    }
  }
}
